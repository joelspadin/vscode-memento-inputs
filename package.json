{
	"name": "memento-inputs",
	"displayName": "Memento Inputs",
	"description": "Create task inputs that remember their last-used values",
	"repository": {
		"type": "git",
		"url": "https://github.com/ChaosinaCan/vscode-memento-inputs"
	},
	"author": {
		"name": "Joel Spadin",
		"email": "joelspadin@gmail.com",
		"url": "https://github.com/ChaosinaCan"
	},
	"publisher": "spadin",
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:memento.pickString",
		"onCommand:memento.promptString"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "memento.pickString",
				"title": "Pick String"
			},
			{
				"command": "memento.promptString",
				"title": "Prompt String"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memento.pickString",
					"when": "false"
				},
				{
					"command": "memento.promptString",
					"when": "false"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src/**/*.ts",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^10.17.21",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-prettier": "^3.1.3",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3",
		"vsce": "^1.75.0",
		"vscode-test": "^1.3.0"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"tabWidth": 4,
		"printWidth": 120,
		"endOfLine": "auto"
	}
}
